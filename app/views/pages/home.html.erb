<!-- ABOUT SECTION -->


<div id="about" class="trans">
  <div class="container">
    <%= render 'layouts/navbar' %>
    <div class="section-title text-center center">
      
    </div>
    <div class="row">
      <div class=""></div>
      <div class="col-md-8 col-md-offset-2 text-center"><p></p>
        <div class="about-text">
          	<% @details.each do |detail| %>
               <div class="col-md-12 text-center">
                   <h2><%= detail.name %>(Test Version)</h2>
                   <h4>We Specialise in Microsoft Cloud Architecture</h4> <br />
                   <p class="detail-pic"><%= image_tag detail.picture.url if detail.picture?  %></p>
                   <h5><%= detail.employee_name%> - <%= detail.title %> </h5>
                   <p><%= simple_format detail.description %></p><br />
                  <br />
                 </div>
             <% end %>
          <p class="text-center"><a class="btn btn-primary res" href="#resume"><i class="fa fa-download"></i> Download Resume</a></p>
        </div>
      </div>
    </div>
  </div>
</div> <br /><br />

